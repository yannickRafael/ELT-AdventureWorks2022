                         (Production.WorkOrder.WorkOrderID IS NOT NULL) AND (Production.WorkOrderRouting.OperationSequence IS NOT NULL) AND (Production.WorkOrder.ScrappedQty IS NOT NULL) AND 
                         (Production.WorkOrder.EndDate IS NOT NULL) AND (CONVERT(int, Production.WorkOrder.EndDate) IS NOT NULL) AND (Production.WorkOrder.StockedQty IS NOT NULL) AND (Production.Product.Color IS NOT NULL) AND 
