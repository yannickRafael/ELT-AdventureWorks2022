                      dataType="System.String"
                      description="The SQL command to be executed."
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT        Production.Location.LocationID AS locationId, Production.Location.Name AS locationName, Production.Product.ProductID AS productId, Production.Product.Name AS productName, 
                         Production.WorkOrder.WorkOrderID AS workOrderId, Production.WorkOrderRouting.OperationSequence AS operationSequence, Production.WorkOrder.ScrappedQty AS scrappedQty, Production.WorkOrder.EndDate AS endDate, 
                         CONVERT(int, Production.WorkOrder.EndDate) AS dataId, Production.WorkOrder.StockedQty AS stockedQty, Production.Product.Color AS color, Production.Product.MakeFlag AS makeFlag, Production.Product.ListPrice AS listPrice, 
                         Production.Product.[Size] AS [size], Production.Product.Weight AS weight, Production.Product.SizeUnitMeasureCode AS sizeUnitMeasureCode, Production.Product.WeightUnitMeasureCode AS weightUnitMeasureCode, 
                         Production.Product.DaysToManufacture AS daysToManufacture, Production.Product.ProductLine AS productLine, Production.Product.Class AS class, Production.Product.Style AS style, 
                         Production.ScrapReason.Name AS scrapReason
FROM            Production.Product INNER JOIN
                         Production.WorkOrder ON Production.Product.ProductID = Production.WorkOrder.ProductID INNER JOIN
                         Production.WorkOrderRouting ON Production.WorkOrder.WorkOrderID = Production.WorkOrderRouting.WorkOrderID INNER JOIN
                         Production.Location ON Production.WorkOrderRouting.LocationID = Production.Location.LocationID INNER JOIN
                         Production.ScrapReason ON Production.WorkOrder.ScrapReasonID = Production.ScrapReason.ScrapReasonID
